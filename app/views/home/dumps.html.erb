<h1><%= t(".heading") %></h1>

<table>
  <thead>
    <tr>
      <th><%= Dump.human_attribute_name(:name) %></th>
      <th><%= Dump.human_attribute_name(:path) %></th>
      <th><%= Dump.human_attribute_name(:files) %></th>
      <th><%= Dump.human_attribute_name(:updated_at) %></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @dumps.each do |dump| %>
      <tr>
        <td><%= dump.name %></td>
        <td><%= link_to dump_path(dump), dump_path(dump) %></td>
        <td><%= dump.files.length %></td>
        <td><%= time_tag dump.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to_back %>
